@echo off
REM Kill some less important processes Win v1
@title Kill some less important processes Win v1
timeout /t 1 >NUL
echo.
echo v1
echo Ensure you run as ADMIN!
timeout /t 1 >NUL
echo Killing some extra processes that aren't entirely strictly necessary...
timeout /t 1 >NUL
echo.
taskkill /f /im steamwebhelper.exe
taskkill /f /im Taskmgr.exe
taskkill /f /im Skype.exe
taskkill /f /im GameOverlayUI.exe
taskkill /f /im "Everything.exe"
taskkill /f /im "wallpaper32.exe"
taskkill /f /im "flux.exe"
taskkill /f /im "msedge.exe"
taskkill /f /im "SearchIndexer.exe"
taskkill /f /im "Launchy.exe"
taskkill /f /im "unchecky_bg.exe"
timeout /t 1 >NUL
echo.
echo Cleaned up, enjoy your performance!
echo.
timeout /t 1 >NUL
echo To close,
pause
