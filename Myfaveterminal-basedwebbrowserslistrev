# My fave terminal-based web browsers list rev. 2

w3m
sudo apt install -y w3m w3m-img

links
sudo apt install -y links

links2
sudo apt install -y links2

elinks
sudo apt install -y elinks

browsh

# I recommend using byobu OR tmux while using those, for further ease of use
byobu
sudo apt install -y byobu

tmux
sudo apt install -y tmux


# Install all of the above (Ubuntu commands);
sudo apt install -y w3m w3m-img links links2 elinks byobu tmux
cd /tmp
wget https://github.com/browsh-org/browsh/releases/download/v1.6.4/browsh_1.6.4_linux_amd64.deb
sudo gdebi -n browsh_1.6.4_linux_amd64.deb
